    <div class="text-center">
      <img src="/{{user._id}}/{{user._id}}.jpg" class="user one reloadimage" alt="profile photo">
      <div class="rank-label-container">
        <span class="label label-default rank-label">{{user.name}}</span>
      </div>
      <small>Upload a different photo...</small>
      {{!-- <form id="uploadform" class="form-horizontal" action="/myprofile/uploadphoto" method="POST" enctype="multipart/form-data"> --}}

      <form id="uploadform" class="form-horizontal" enctype="multipart/form-data">
        <div class="form-group">
          <input type="file" name="displayImage" id="displayImage" />
        </div>

        <div class="form-group">
            <button type="submit" class="btn btn-info">Upload</button>
            <button type="reset" class="btn btn-info">Cancel</button>
        </div>

        <div class="ErrorPanel hidden">
      </div>

        <input type="hidden" id="_id" name="_id" value={{user._id}} />

      </form>
    </div>