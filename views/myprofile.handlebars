<div class="container-fluid">
  <div class="row">

    <!--First column-->
    <div class="col-lg-4">
      {{> uploadprofilephoto}}
    </div>
    <!--/.First column-->

    <!--Second column-->
    <div class="col-lg-8 col-md-8">
      <div class="widget-wrapper">
        <div class="list-group">
          <h4 class="list-group-item active">About me</h4>
          <span class="list-group-item">
            This is about me and my proffession.
          </span>
        </div>
      </div>

      <div class="row">
        <!--Second1 column-->
        <div class="col-lg-6 col-md-6">
          <div class="widget-wrapper wow fadeIn" data-wow-delay="0.4s">
            <div class="list-group">
              <h4 class="list-group-item active">Personal info</h4>
              <div class="list-group-item">
                <div class="PDErrorPanel hidden">
                </div>
                <form class="form-horizontal" action="" method="">
                  <div class="form-group md-form">
                    <label for="firstname">First Name</label>
                    <input type="text" class="form-control" id="firstname" name="firstname" required="required" autofocus="autofocus">
                  </div>

                  <div class="form-group md-form">
                    <label for="lastname">Last Name</label>
                    <input type="text" class="form-control" id="lastname" name="lastname" required="required">
                  </div>

                  <div class="form-group md-form">
                    <label for="country">Country</label>
                    <input type="text" class="form-control" id="country" name="country" required="required">
                  </div>

                  <div class="form-group md-form">
                    <label for="phone">Contact Number</label>
                    <input type="text" class="form-control" id="phone" name="phone" required="required">
                  </div>

                  <div class="form-group ">
                    <button class="btn btn-info" type="submit">Save</button>
                    <button class="btn btn-info" type="reset">Cancel</button>
                  </div>

                </form>
              </div>


            </div>
          </div>
        </div>
        <!--/.Second column-->

        <!--Third1 column-->
        <div class="col-lg-6 col-md-6">
          <div class="widget-wrapper wow fadeIn" data-wow-delay="0.4s">
            <div class="list-group">
              <h4 class="list-group-item active">Proffessional details</h4>
              <div class="list-group-item">
                <div class="EduErrorPanel hidden">
                </div>
                <form class="form-horizontal" action="" method="">
                  <div class="form-group md-form">
                    <label for="proffession">Proffession</label>
                    <input type="text" class="form-control" id="proffession" name="proffession" required="required" autofocus="autofocus">
                  </div>

                  <div class="form-group md-form">
                    <label for="department">Department</label>
                    <input type="text" class="form-control" id="department" name="department" required="required">
                  </div>

                  <div class="form-group md-form">
                    <label for="company">Company</label>
                    <input type="text" class="form-control" id="company" name="companys`s" required="required">
                  </div>

                  <div class="form-group md-form">
                    <label for="location">Experience</label>
                    <input type="text" class="form-control" id="location" name="location" required="required">
                  </div>

                  <div class="form-group ">
                    <button class="btn btn-info" type="submit">Save</button>
                    <button class="btn btn-info" type="reset">Cancel</button>
                  </div>

                </form>
              </div>
            </div>
          </div>
        </div>
        <!--/.Third column-->
      </div>

