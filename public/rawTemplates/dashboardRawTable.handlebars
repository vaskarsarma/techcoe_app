<div class="body table-responsive">
    <table class="table table-hover table-striped userDataTable">
        <thead>
            <tr>
                <th>USER NAME</th>
                <th>NAME</th>
                <th>EMAIL</th>
                <th>EMAIL VERIFIED</th>
                <th>ACTIVE</th>
                <th>ADMIN</th>
                <th>CREATION DATE</th>
            </tr>
        </thead>
        <tbody>
            {{#each user}}
            <tr>
                <td>{{username}}</td>
                <td>{{name}}</td>
                <td>{{email}}</td>
                <td>{{IsEmailVerified}}</td>
                <td>{{active}}</td>
                <td>{{admin}}</td>
                <td>{{dateTime}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>


