<div class="body table-responsive">
    <table id="userDataTable" class="table table-hover table-striped userDataTableClass">
        <thead>
            <tr>
                {{!--
                <th>USER NAME</th> --}}
                <th>NAME</th>
                <th>EMAIL</th>
                <th>EMAIL VERIFIED</th>
                <th>ACTIVE</th>
                <th>ADMIN</th>
                <th>CREATION DATE</th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            {{#each user}}
            <tr>
                {{!--
                <td>{{username}}</td> --}}
                <td>{{name}}</td>
                <td>{{email}}</td>
                <td>
                    {{#if IsEmailVerified}}
                    <input type="checkbox" checked> {{else}}
                    <input type="checkbox"> {{/if}}
                </td>
                <td>
                    {{#if active}}
                    <input type="checkbox" checked> {{else}}
                    <input type="checkbox"> {{/if}}
                </td>
                <td>
                    {{#if admin}}
                    <input type="checkbox" checked> {{else}}
                    <input type="checkbox"> {{/if}}

                </td>
                <td>{{dateTime}}</td>
                <td> <button type="button" class="btn btn-info waves-effect userDataColumn">Update</button></td>

            </tr>
            {{/each}}
        </tbody>
    </table>
</div>


